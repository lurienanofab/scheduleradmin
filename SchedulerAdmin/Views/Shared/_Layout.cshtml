@{
    string tab = ViewBag.ActiveTab;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>@ViewBag.Title</title>

    <!-- Bootstrap -->
    <link href="//ssel-apps.eecs.umich.edu/static/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <link href="~/Content/Site.css" rel="stylesheet" />

    @RenderSection("styles", false)
</head>
<body>
    <div class="container-fluid">
        <div class="page-header">
            <h1>Scheduler Admin</h1>
        </div>

        <ul class="nav nav-tabs">
            <li role="presentation" class="@(tab == "activities" ? "active" : "")">@Html.ActionLink("Activites", "Index", "Activities")</li>
            <li role="presentation" class="@(tab == "buildings" ? "active" : "")">@Html.ActionLink("Buildings", "Index", "Buildings")</li>
            <li role="presentation" class="@(tab == "labs" ? "active" : "")">@Html.ActionLink("Labs", "Index", "Labs")</li>
            <li role="presentation" class="@(tab == "proctechs" ? "active" : "")">@Html.ActionLink("ProcessTechs", "Index", "ProcessTechs")</li>
            <li role="presentation" class="@(tab == "resources" ? "active" : "")">@Html.ActionLink("Resources", "Index", "Resources")</li>
            <li role="presentation" class="@(tab == "properties" ? "active" : "")">@Html.ActionLink("Properties", "Index", "Properties")</li>
        </ul>

        <div style="margin-top: 30px;">
            @RenderBody()
        </div>
    </div>

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="//ssel-apps.eecs.umich.edu/static/lib/jquery/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="//ssel-apps.eecs.umich.edu/static/lib/bootstrap/js/bootstrap.min.js"></script>

    @RenderSection("scripts", false)
</body>
</html>